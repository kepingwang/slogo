<?xml version="1.0" encoding="UTF-8"?>

<!-- You have to import packages manually in FXML!!! -->
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>


<AnchorPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8"
	fx:controller="frontend.help.BrowserController">
	<children>
		<HBox>
			<children>
				<Button text="previous" fx:id="prevBtn" onAction="#prevPage" />
				<Button text="next" fx:id="nextBtn" onAction="#nextPage" />
				<TextField fx:id="addressField" prefWidth="500" />
				<Button text="go" onAction="#goToAddress" />
			</children>
		</HBox>
		<WebView fx:id="browser" 
			AnchorPane.topAnchor="30.0" AnchorPane.bottomAnchor="0" 
			AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" />
	</children>
</AnchorPane>